pipeline {
    agent any
    tools{
        maven 'M2_HOME'
    }
    stages{
        stage("checkout scm"){
            steps {
                checkout scm
            }
        }
        stage("sonarcloud-analysis"){
            steps{
                withSonarQubeEnv ('sonarqube'){
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=Hossnia_geo1'
            }
            }
            
        }
        stage("maven-build"){
            steps{
                sh 'mvn clean; mvn validate; mvn compile; mvn install; mvn package; mvn test'
            }
        }
    }
}